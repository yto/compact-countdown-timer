{
    "manifest_version": 3,
    "name": "コンパクトカウントダウンタイマー",
    "version": "0.0.1",
    "description": "任意の時間で動作するシンプルなカウントダウンタイマー",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    }
}
